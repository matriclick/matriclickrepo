<%= content_for :products_and_services_page_in_content do %>
	<% if @object.video.blank? -%>
		<%= render "layouts/nothing_to_show", :objects => "multimedia", :small => true %>
	<% else -%>
		<iframe width="420" height="315" src="http://www.youtube.com/embed/<%= @object.video.url_code%>" frameborder="0" allowfullscreen></iframe>
	<% end -%>
<% end %>

<%= render "products_and_services_page" %>
